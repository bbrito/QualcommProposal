journal = {Swarm Intelligence},
keywords = {Review,Swarm engineering,Swarm robotics},
mendeley-groups = {Multi Robot motion Planning},
number = {1},
pages = {1--41},
title = {{Swarm robotics: A review from the swarm engineering perspective}},
volume = {7},
year = {2013}
}

